{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "myvenv",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",

            "env": {
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1",
                "GEVENT_SUPPORT":"False",
                "DISPLAY":":99"
            }
        }
    ]
}
